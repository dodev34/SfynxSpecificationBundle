{
  "name": "sfynx-project/tool-specification-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides handlers to rapidly develop with the specification pattern in DDD applicative architecture",
  "keywords": ["ddd", "pattern", "specification"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0+",

  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxSpecificationBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.3|~3.0"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\SpecificationBundle": "" }
  },

  "target-dir": "Sfynx/SpecificationBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.8-dev"
    }
  }
}
